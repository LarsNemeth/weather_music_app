eather ${weatherStatus ? 'active-weather' : ''}`}>\n      <div className=\"weather-app\">\n        <div className={bgChange()}>\n          <div className=\"search-box\">\n            <input\n              type=\"text\"\n              className=\"search-bar\"\n              placeholder=\"City Sounds\"\n              onChange={onChangeCity}\n            />\n            <button type=\"button\" onClick={onClickButton}>\n              Search\n            </button>\n          </div>\n          {weatherdata != null ? (\n            <div className=\"weather-wrap\">\n              <div className=\"location-box\">\n                <div className=\"location\">\n                  {weatherdata.name} | {weatherdata.sys.country}\n                </div>\n                <div className=\"date\">{dateBuilder()}</div>\n              </div>\n              <div className=\"weather-box\">\n                <div className=\"temp\">\n                  {parseFloat(weatherdata.main.temp - 273.15).toFixed(0)}Â°C\n                </div>\n                <div className=\"weather-condition\">\n                  {weatherdata.weather[0].main}\n                </div>\n              </div>\n            </div>\n          ) : null}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Weather;\n","/Users/Larsbook/Desktop/DCI/## Projekte/weather_music_app/src/components/LibrarySong.js",[],"/Users/Larsbook/Desktop/DCI/## Projekte/weather_music_app/src/services/apiKeys.js",[],{"ruleId":"48","severity":1,"message":"49","line":1,"column":27,"nodeType":"50","messageId":"51","endLine":1,"endColumn":36},"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar"]